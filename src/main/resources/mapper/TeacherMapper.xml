<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.zhku.boot.mapper.TeacherMapper">

    <resultMap id="BaseResultMap" type="edu.zhku.boot.entity.Teacher">
            <id property="teacherId" column="teacher_id" jdbcType="BIGINT"/>
            <result property="collegeId" column="college_id" jdbcType="BIGINT"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="gender" column="gender" jdbcType="TINYINT"/>
            <result property="birth" column="birth" jdbcType="TIMESTAMP"/>
            <result property="remark" column="remark" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        teacher_id,college_id,name,
        college,gender,birth,
        remark
    </sql>
    <select id="getNameById" resultType="java.lang.String">
        SELECT student_achievement.college.name FROM student_achievement.college
    </select>
</mapper>
